{
  "name": "synthlet",
  "version": "0.1.0",
  "description": "Synthesizer for Web Audio API",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/audioworklet": "^0.0.38",
    "@types/jest": "^29.0.3",
    "jest": "^29.0.3",
    "standardized-audio-context-mock": "^9.6.3",
    "ts-jest": "^29.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "vite": "^3.2.1"
  },
  "scripts": {
    "start": "vite example/",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "worklet": "esbuild src/synthlet-processor.ts --bundle --minify > src/processor.js"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
